//ex2 create a function that will take 3 parameters from a spread operator
let ironGuitars = ['Gears', 'McBrain', 'Smith']
function ironMaiden(a,b,c){
    return `Iron's guitars are played by ${a}, ${b} and ${c}`
}
ironMaiden(...ironGuitars);
