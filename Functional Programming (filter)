//pratice for functional programming

var weather = [72,84,34,56,92,24,47,85,72,54]

var goOutside = []

for(var i = 0; i < weather.length; i++){
    if(weather[i]>70){
        goOutside.push(weather[i]);
    }
}
goOutside;




//agora usando a função 'filter'

var niceWeaather = function(temp){
    return temp > 70
}
var goOutside = weather.filter(niceWeaather)
